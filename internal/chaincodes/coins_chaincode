package main

import (
	"fmt"

	"github.com/hyperledger/fabric-contract-api-go/contractapi"
	c "github.com/rafa-mori/smart_plane/internal/smart_contracts/coins"
)

type CoinsChaincode struct {
	// CoinBase Wrapping by CoinsChaincode
	CoinBase c.CoinBase

	// Another Coins Wrapping can be added here at the same chaincode
}

func main() {
	chaincode, err := contractapi.NewChaincode(new(CoinsChaincode))
	if err != nil {
		fmt.Printf("Erro ao criar Chaincode: %v", err)
		return
	}

	if err := chaincode.Start(); err != nil {
		fmt.Printf("Erro ao iniciar Chaincode: %v", err)
	}
}
